{
    "name": "symfony/doctrine-scheduler",
    "type": "symfony-bridge",
    "description": "Symfony Doctrine Scheduler Bridge",
    "keywords": ["symfony", "bridge", "schedule", "scheduler", "doctrine"],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Loulier",
            "email": "contact@guillaumeloulier.fr"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/scheduler": "^5.3"
    },
    "require-dev": {
        "doctrine/common": "^2.13|^3.0",
        "doctrine/dbal": "^2.10|^3.0",
        "doctrine/persistence": "^1.3|^2.0",
        "symfony/event-dispatcher": "^5.2",
        "symfony/serializer": "^5.2"
    },
    "conflict": {
        "doctrine/common": "<2.13",
        "doctrine/dbal": "<2.8",
        "doctrine/persistence": "<1.3"
    },
    "autoload": {
        "psr-4": { "Symfony\\Component\\Scheduler\\Bridge\\Doctrine\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.3"
        }
    }
}
